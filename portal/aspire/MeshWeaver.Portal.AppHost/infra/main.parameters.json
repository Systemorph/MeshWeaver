{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "EntraIdAdminGroupId": {
        "value": "${AZURE_ENTRA_ID_ADMIN_GROUP_ID}"
      },
      "EntraIdClientId": {
        "value": "${AZURE_ENTRA_ID_CLIENT_ID}"
      },
      "EntraIdTenantId": {
        "value": "${AZURE_ENTRA_ID_TENANT_ID}"
      },
      "mesh_cluster_id": {
        "value": "${AZURE_MESH_CLUSTER_ID}"
      },
      "mesh_service_id": {
        "value": "${AZURE_MESH_SERVICE_ID}"
      },
      "meshweaverCertificate": {
        "value": "${AZURE_MESHWEAVER_CERTIFICATE}"
      },
      "meshweaverDomain": {
        "value": "${AZURE_MESHWEAVER_DOMAIN}"
      },
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      }
    }
  }
  