{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MeshWeaver": "Information",
      "MeshWeaver.Messaging.MessageService": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "EntraId": {
    "Domain": "meshweaverportal.onmicrosoft.com",
    "CallbackPath": "/authentication/login-callback",
    "PostLogoutRedirectUri": "/authentication/logout-callback",
    "Scopes": "openid profile email",
    "Groups": {
      "GroupClaimType": "groups",
      "AllowedGroupIds": [],
      "RoleMappings": {
      }
    }
  },
  "Styles": {
    "StylesheetName": "MeshWeaver.Portal.Orleans"
  },
  "ArticleCollections": [
    {
      "SourceType": "AzureBlob",
      "Name": "Documentation",
      "DisplayName": "Documentation",
      "BasePath": "documentation"
    },
    {
      "SourceType": "AzureBlob",
      "Name": "Reinsurance",
      "DisplayName": "Reinsurance",
      "BasePath": "reinsurance"
    }
  ]
}