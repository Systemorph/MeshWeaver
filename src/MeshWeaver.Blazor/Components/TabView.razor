@inherits SkinnedView<TabSkin, TabView>

<FluentTab Class="@Class" Style="@Style" Label="@SkinLabel">     
    <DispatchView ViewModel="@ViewModel" Stream="@Stream" Area="@Area" />
</FluentTab>

@code{
    private string SkinLabel { get; set; }
    protected override void BindData()
    {
        base.BindData();
        DataBind(Skin.Label, x => x.SkinLabel);
    }

}