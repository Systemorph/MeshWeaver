{
  "$type": "MeshWeaver.Charting.Models.ChartModel",
  "data": {
    "$type": "MeshWeaver.Charting.Models.ChartData",
    "datasets": [
      {
        "$type": "MeshWeaver.Charting.Models.Bar.BarDataSet",
        "type": "Bar",
        "data": [
          1,
          2,
          3,
          4
        ],
        "label": "First"
      },
      {
        "$type": "MeshWeaver.Charting.Models.Bar.BarDataSet",
        "type": "Bar",
        "data": [
          5,
          6,
          7,
          8
        ],
        "label": "Second"
      }
    ],
    "labels": [
      "One",
      "Two",
      "Three",
      "Four"
    ]
  },
  "options": {
    "$type": "MeshWeaver.Charting.Models.Options.ChartOptions",
