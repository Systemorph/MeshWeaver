      {
        "$type": "MeshWeaver.Charting.Models.Line.ScatterDataSet",
        "type": "Scatter",
        "data": [
          {
            "x": -10,
            "y": 0
          },
          {
            "x": 0,
            "y": 10
          },
          {
            "x": 10,
            "y": 5
          }
        ]
      }
